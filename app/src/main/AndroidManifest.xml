<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.ch.wit">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ProfileActivity"
            android:label="@string/title_activity_profile"></activity>
        <activity android:name=".Feeds.DetailedPostActivity" />
        <activity
            android:name=".SplashActivity"
            android:label="@string/title_activity_feeds" />
        <activity
            android:name=".AuthActivity"
            android:label="@string/title_activity_splash_activity" />
        <activity android:name=".Feeds.FeedsActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>